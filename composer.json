{
    "name": "ministryofjustice/jwt-zend-auth",
    "description": "JWT authentication module for ZF2/3",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^2.3.0 || ^3.4",
        "zendframework/zend-modulemanager": "^2.3.0",
        "zendframework/zend-authentication": "^2.3.0",
        "zendframework/zend-http": "^2.3.0",
        "zendframework/zend-console": "^2.3.0",
        "zendframework/zend-mvc": "^2.3.0 || ^3.1",
        "lcobucci/jwt":"~3.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JwtZendAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JwtZendAuthTest\\": "test/"
        }
    }
}
